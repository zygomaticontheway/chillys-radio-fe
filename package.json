{
  "name": "vite-template-redux",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "21"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d build",
    "lazy": "git add . && git commit -m 'node version changed to 21 in Dockerfile' && git push"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "@types/node": "^22.7.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.26.2",
    "typescript": "^5.6.2",
    "use-debounce": "^10.0.3",
    "uuid": "^10.0.0",
    "vitest": "^1.2.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/react": "^18.2.47",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.1",
    "vite": "^5.4.1",
    "web-vitals": "^2.1.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
